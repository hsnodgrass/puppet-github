{
    "name": "puppet-github",
    "version": "0.0.1",
    "author": "Puppet Inc.",
    "summary": "Configures GitHub mirrors",
    "license": "Apache-2.0",
    "source": "https://github.com/underscorgan/puppet-github",
    "project_page": "https://github.com/underscorgan/puppet-github",
    "dependencies": [
        { "name": "puppetlabs/apache", "version_requirement": "5.2.0" }
    ],
    "operatingsystem_support": [
        {
            "operatingsystem": "RedHat",
            "operatingsystemrelease": [
                "5",
                "6",
                "7",
                "8"
            ]
        },
        {
            "operatingsystem": "CentOS",
            "operatingsystemrelease": [
                "5",
                "6",
                "7"
            ]
        },
        {
            "operatingsystem": "OracleLinux",
            "operatingsystemrelease": [
                "5",
                "6",
                "7"
            ]
        },
        {
            "operatingsystem": "Scientific",
            "operatingsystemrelease": [
                "6",
                "7"
            ]
        },
        {
            "operatingsystem": "Debian",
            "operatingsystemrelease": [
                "8",
                "9",
                "10"
            ]
        },
        {
            "operatingsystem": "SLES",
            "operatingsystemrelease": [
                "11 SP1",
                "12",
                "15"
            ]
        },
        {
            "operatingsystem": "Ubuntu",
            "operatingsystemrelease": [
                "16.04",
                "18.04"
            ]
        }
    ],
    "requirements": [
        {
            "name": "puppet",
            "version_requirement": ">= 5.5.10 < 7.0.0"
        }
    ]
}